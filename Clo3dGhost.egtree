                            <Macro Name="Auto-unzip" XML_Guid="{96BE90F3-0514-4E03-BA57-D9D54EABFE51}">
                                <Event Name="DirectoryWatcher.Created" XML_Guid="{0057B84D-9861-47B2-B65B-F2FAB72FDDC7}" />
                                <Action Name="Auto extract compressed folder" XML_Guid="{37B66BA0-175C-4097-B4DA-95CD13DA6981}">
                                    EventGhost.PythonScript(u'import os\nimport shutil\nfrom zipfile import ZipFile\nfrom zipfile import BadZipfile\n#Shorten effort of creating eg events\n#Note eg.globals still needs full name if called from within plugin event.\negg = eg.globals\negp = eg.plugins\negl = eg.plugins.FileOperations.Write\nege = eg.plugins.System.Execute\n\n#Log file name to egg.logdir\\sortlog.txt\negl(2, u\'{eg.event.payload[0]}\', u\'{eg.globals.logdir}sortlog.txt\', 2, 0, False, True, False, \'cp1252\')\n#Get first object in payload and set as string.\nf = str(eg.event.payload[0])\n\n#Split and name the filename "h" out of the path\ng = os.path.split(f)\nh = str(g[1])\n\n#Setup name of target folder\nj = egg.sorter+h+"\\\\"\n\n#Check file for zip extenshion\nif h[-4:] != ".rar":\n    #Return file to sorter if not compressed.\n    shutil.move(f,egg.sorter)\n    print "Not a compressed file, returning to sorter"\n    egl(2, u\'{eg.event.payload[0]} - Returned to sorter\', u\'{eg.globals.logdir}sortlog.txt\', 2, 0, False, True, False, \'cp1252\')\n    exit()\n\n#Test to see if directory exists. This check is likely unneeded.\nelse:\n    try:\n        os.makedirs(j)\n    except WindowsError:\n        pass\n\n#Unzip file\ntry:\n    with ZipFile(f, \'r\') as zipObj:\n        zipObj.extractall(j)\n#prevent deletion of non-zip folders\nexcept BadZipfile:\n    #Log Error and Open folder\n    egl(2, u\'{eg.event.payload[0]} - Badzipfile\', u\'{eg.globals.logdir}sortlog.txt\', 2, 0, False, True, False, \'cp1252\')\n    ege(u\'{eg.globals.sorter}Unzipper\', u\'\', 0, False, 2, u\'\', False, False, u\'\', False, False, False, False)\n    exit()\n\n#Once done, remove file.\nif os.path.exists(f):\n    egl(2, u\'{eg.event.payload[0]} - Successful decompress\', u\'{eg.globals.logdir}sortlog.txt\', 2, 0, False, True, False, \'cp1252\')\n    os.remove(f)\n')
                                </Action>
                            </Macro>
